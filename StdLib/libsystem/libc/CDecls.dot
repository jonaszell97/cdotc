
import builtin

alias void               = builtin.CVoid
alias char               = builtin.u8
alias int                = builtin.i32
alias unsigned           = builtin.u32
alias short              = builtin.i16
alias unsigned_short     = builtin.u16
alias long               = builtin.i64
alias unsigned_long      = builtin.u64
alias long_long          = builtin.i64
alias unsigned_long_long = builtin.u64

alias size_t             = long_long
alias ssize_t            = unsigned_long_long
alias off_t              = long_long
alias time_t             = long

alias cstring            = builtin.RawPointer<char>