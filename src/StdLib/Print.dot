
using Extern.C.printf
using Builtin.Primitive.{ int8, int64, double }

def print() {
    printf("\n")
}

def print(s: String, separator: int8 = '\n') {
    printf("%s%c", s.charArray, separator)
}

def print(i: int64, separator: int8 = '\n') {
    printf("%li%c", i, separator)
}

def print(d: double, separator: int8 = '\n') {
    printf("%f%c", d, separator)
}

def println <T, U, ...V> (t: T, u: U, v: V...) {
    print(std.toString(t))
    println(u, v...)
}

def println <T> (t: T) {
    print(std.toString(t))
}
