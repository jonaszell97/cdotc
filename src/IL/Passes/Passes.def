
#ifndef CDOT_PASS
#  define CDOT_PASS(CLASS, NAME)
#endif

#ifndef CDOT_FUNCTION_PASS
#  define CDOT_FUNCTION_PASS(CLASS, NAME) CDOT_PASS(CLASS, NAME)
#endif

#ifndef CDOT_MODULE_PASS
#  define CDOT_MODULE_PASS(CLASS, NAME) CDOT_PASS(CLASS, NAME)
#endif

#ifndef CDOT_MANDATORY_PASS
#  define CDOT_MANDATORY_PASS(CLASS, NAME) CDOT_PASS(CLASS, NAME)
#endif

#ifndef CDOT_MANDATORY_FUNCTION_PASS
#  define CDOT_MANDATORY_FUNCTION_PASS(CLASS, NAME) CDOT_MANDATORY_PASS(CLASS, NAME)
#endif

#ifndef CDOT_MANDATORY_MODULE_PASS
#  define CDOT_MANDATORY_MODULE_PASS(CLASS, NAME) CDOT_MANDATORY_PASS(CLASS, NAME)
#endif

#ifndef CDOT_ABSTRACT_PASS
#  define CDOT_ABSTRACT_PASS(CLASS)
#endif

CDOT_ABSTRACT_PASS(_firstFunctionPass)
   CDOT_MANDATORY_FUNCTION_PASS(ReorderBasicBlockPass, "reorder-basic-blocks")
   CDOT_MANDATORY_FUNCTION_PASS(DefinitiveInitializationPass, "definitive-init")
   CDOT_MANDATORY_FUNCTION_PASS(BorrowCheckPass, "borrow-chk")
   CDOT_MANDATORY_FUNCTION_PASS(FinalizeFunctionPass, "finalize-func")

   CDOT_FUNCTION_PASS(VerifierPass, "verify")
   CDOT_FUNCTION_PASS(StackPromotion, "stack-promotion")
CDOT_ABSTRACT_PASS(_lastFunctionPass)

CDOT_ABSTRACT_PASS(_firstModulePass)
CDOT_ABSTRACT_PASS(_lastModulePass)

#undef CDOT_PASS
#undef CDOT_FUNCTION_PASS
#undef CDOT_MODULE_PASS

#undef CDOT_MANDATORY_PASS
#undef CDOT_MANDATORY_FUNCTION_PASS
#undef CDOT_MANDATORY_MODULE_PASS

#undef CDOT_ABSTRACT_PASS